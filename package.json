{
  "name": "@meza/typescript-react-template",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/types",
  "private": true,
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --mode=production",
    "start": "webpack serve",
    "ci": "yarn ci:dev",
    "ci:dev": "yarn lint && yarn test && yarn build",
    "ci:full": "yarn lint && yarn report && yarn build",
    "clean": "rm -rf .cache dist reports yarn-error.log storybook-static build-storybook.log",
    "clean:all": "yarn clean && rm -rf node_modules",
    "lint:eslint": "eslint --ext \".tsx,.jsx,.ts,.js,.json\" --cache --cache-location \".cache/\" src cypress",
    "lint:ts": "tsc",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:ts",
    "lint": "yarn lint:eslint && yarn lint:ts",
    "test": "jest",
    "test:e2e": "CYPRESS_CACHE_FOLDER=~/.cache/Cypress cypress run",
    "test:full": "yarn test && yarn test:e2e",
    "cypress": "CYPRESS_CACHE_FOLDER=~/.cache/Cypress cypress open",
    "report": "jest --coverage && jscpd src",
    "release": "standard-version",
    "postinstall": "is-ci || husky install",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 8081",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "chromatic": "chromatic"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.20",
    "core-js": "^3.19.1",
    "dotenv-defaults": "^5.0.0",
    "history": "^5.1.0",
    "i18next": "^21.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "^0.13.9",
    "sanitize.css": "^13.0.0",
    "workbox-cacheable-response": "^6.4.1",
    "workbox-core": "^6.4.1",
    "workbox-expiration": "^6.4.1",
    "workbox-precaching": "^6.4.1",
    "workbox-routing": "^6.4.1",
    "workbox-strategies": "^6.4.1",
    "workbox-window": "^6.4.1"
  },
  "standard-version": {
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ],
    "packageFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@cypress/webpack-preprocessor": "^5.10.0",
    "@jscpd/badge-reporter": "^3.4.1",
    "@meza/tsconfig-base": "^1.1.0",
    "@storybook/addon-a11y": "^6.4.0",
    "@storybook/addon-docs": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.0",
    "@storybook/api": "^6.4.0",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/components": "^6.4.0",
    "@storybook/core-events": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@storybook/theming": "^6.4.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "chance": "^1.1.8",
    "chromatic": "^6.0.6",
    "clean-webpack-plugin": "^4.0.0",
    "colorguard": "^1.2.1",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.12",
    "cypress": "^9.1.0",
    "cypress-dark": "^1.8.3",
    "cypress-react-selector": "^2.3.13",
    "cypress-timings": "^1.0.0",
    "cypress-wait-until": "^1.7.2",
    "doiuse": "^4.4.1",
    "eslint": "^8.2.0",
    "eslint-config-tailored-tunes": "^5.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-storybook": "^0.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "install-deps-postmerge": "^2.0.1",
    "is-ci": "^3.0.1",
    "jest": "^27.3.1",
    "jest-chance": "^0.1.11",
    "jest-junit": "^13.0.0",
    "jest-localstorage-mock": "^2.4.18",
    "jscpd": "^3.4.2",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-loader": "^6.2.1",
    "postcss-mixins": "^9.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.0.0",
    "postcss-scss": "^4.0.2",
    "postcss-sorting": "^7.0.1",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "standard-version": "^9.3.2",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "workbox-webpack-plugin": "^6.4.1",
    "yarn": "^1.22.17"
  },
  "packageManager": "yarn@3.1.0",
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "jest-junit": {
    "outputDirectory": "reports/coverage",
    "outputName": "./junit.xml"
  },
  "jscpd": {
    "output": "./reports/",
    "threshold": 20,
    "reporters": [
      "html",
      "console",
      "badge"
    ],
    "ignore": [
      "**/__snapshots__/**"
    ],
    "absolute": true,
    "gitignore": true
  },
  "jest": {
    "testEnvironment": "node",
    "timers": "fake",
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "globalSetup": "jest-chance",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "d.ts"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testMatch": [
      "**/?(*.)test.ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!node_modules/**/*",
      "!src/index.tsx",
      "!src/service-worker.js",
      "!src/serviceWorkerRegistration.ts",
      "!src/App/app.i18n.ts",
      "!src/StateManagement/Store/configureStore.ts",
      "!**/*.d.{ts,tsx}"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "cacheDirectory": "./.cache/jest",
    "globals": {
      "window": {}
    }
  }
}
