{
  "name": "@meza/typescript-react-template",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/types",
  "private": true,
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --mode=production",
    "start": "webpack serve",
    "ci": "yarn ci:dev",
    "ci:dev": "yarn lint && yarn build",
    "clean": "rm -rf dist .cache",
    "clean:all": "yarn clean && rm -rf .yarn/cache",
    "lint:eslint": "eslint --ext \".tsx,.jsx,.ts,.js,.json\" --cache --cache-location \".cache/\" src",
    "lint:ts": "tsc",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:ts",
    "lint": "yarn lint:eslint && yarn lint:ts",
    "test": "exit 0",
    "report": "exit 0",
    "release": "standard-version",
    "postinstall": "is-ci || husky install"
  },
  "dependencies": {
    "core-js": "^3.19.1",
    "i18next": "^21.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.2",
    "regenerator-runtime": "^0.13.9",
    "workbox-cacheable-response": "^6.3.0",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-window": "^6.3.0"
  },
  "standard-version": {
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ],
    "packageFiles": [
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@meza/tsconfig-base": "^1.1.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "colorguard": "^1.2.1",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.10",
    "doiuse": "^4.4.1",
    "eslint": "^8.2.0",
    "eslint-config-tailored-tunes": "^5.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.27.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "install-deps-postmerge": "^2.0.1",
    "is-ci": "^3.0.1",
    "mini-css-extract-plugin": "^2.4.4",
    "postcss": "^8.3.11",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.0",
    "postcss-mixins": "^8.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^4.0.2",
    "postcss-sorting": "^7.0.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "workbox-webpack-plugin": "^6.3.0",
    "yarn": "^1.22.17"
  },
  "packageManager": "yarn@3.1.0",
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  }
}
